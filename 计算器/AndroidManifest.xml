<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:name="com.example.calculator.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_calculator"
        android:label="@string/app_name"
        android:theme="@style/BlueTheme" >
        <activity
            android:name="com.example.calculator.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.calculator.SelectThemeActivity"></activity>
        <activity android:name="com.example.calculator.AboutMeActivity"></activity>
        <activity android:name="com.example.calculator.InstructionActivity"></activity>
        <activity android:name="com.example.calculator.SettingActivity"></activity>
        
    </application>

</manifest>
